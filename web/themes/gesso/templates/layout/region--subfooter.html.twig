{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region <div>.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{% embed '@layouts/subfooter/subfooter.twig' with {
  'has_constrain': true,
} %}
  {% block subfooter_content %}
    {{ content }}
    <p>&copy; {{ 'now'|date('Y') }} SLAC National Accelerator Laboratory is operated by Stanford University for the U.S. Department of Energy Office of Science.</p>
  {% endblock %}

{% endembed %}
